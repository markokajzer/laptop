#!/bin/bash

mkdir -p files/atom/
cp ~/.atom/config.cson    files/atom/
cp ~/.atom/github.cson    files/atom/
cp ~/.atom/init.coffee    files/atom/
cp ~/.atom/keymap.cson    files/atom/
cp ~/.atom/snippets.cson  files/atom/
cp ~/.atom/styles.less    files/atom/

mkdir -p files/git/
cp ~/.gitconfig       files/git/
cp ~/.gitconfig-moqo  files/git/
cp ~/.gitignore       files/git/

mkdir -p files/prezto/
cp ~/.zpreztorc files/prezto/
cp ~/.zprofile  files/prezto/
cp ~/.zshrc     files/prezto/
